{
  "name": "@wnba-fantasy/backend",
  "version": "1.0.0",
  "description": "WNBA Fantasy Analytics Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -b",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "fetch:espn": "tsx src/scripts/fetch-espn-data.ts",
    "fetch:espn:force": "tsx src/scripts/fetch-espn-data.ts --force",
    "setup:database": "tsx src/scripts/setup-database.ts",
    "quick:check": "tsx src/scripts/quick-check.ts",
    "update:scores": "tsx -e \"import('./src/jobs/calculateFantasyScores.js').then(m => m.calculateFantasyScores())\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@wnba-fantasy/shared": "^1.0.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "express-winston": "^4.2.0",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "redis": "^4.6.14",
    "stripe": "^15.9.0",
    "winston": "^3.13.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "tsx": "^4.15.2",
    "typescript": "^5.4.5"
  }
}
